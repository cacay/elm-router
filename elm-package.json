        "elm-lang/navigation": "2.0.0 <= v < 3.0.0",
        "sporto/erl": "11.0.0 <= v < 12.0.0",
        "maksar/elm-function-extra": "2.0.0 <= v < 3.0.0",
        "elm-community/maybe-extra": "3.0.1 <= v < 4.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
